  "private": true,
  "source": "index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "build": "parcel build --dist-dir dist",
    "clean": "rm -rf .parcel-cache/ dist/*",
    "dev.server": "nodemon ./main.mjs --ignore dist/*",
