    "csurf": "^1.11.0",
    "dotenv": "^14.3.2",
    "dotenv-expand": "^8.0.1",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "hbs": "^4.2.0",
    "mongoose": "^6.1.8",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "prop-types": "^15.8.1",
